                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        NAF.ID, NAF.SzallitoKod, NAF.KertSzallIdoFej, NAF.MegjegyzesFej, NAF.Kerelmezo, NAF.RendelesDatum, NAS.RendelesSzam, NAS.RendSorszam, NAS.Cikkszam, NAS.RendeltMenny, NAS.Egysegar, 
                         NAS.BeerkMenny, NAS.KertSzallIdo, NAS.Beszerzo, NAS.DatumBeerk, NAS.Engedelyezo, NAS.EngDatum, NAS.Tipus, NAS.Status, NAS.Megjegyzes2, PL.SupplierName, NAS.IDS, SC.Description1, 
                         SC.Description2, SC.InternRem1, SC.InternRem2, NAS.RendeltMenny * NAS.Egysegar AS Ertek, NAS.Beruhazasszam, RTRIM(SCU.Description) AS Mee, NAS.Deviza
FROM            NBAnyagigenyloFej AS NAF LEFT OUTER JOIN
                         NBAnyagigenylo AS NAS ON NAF.ID = NAS.RendelesSzam LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_PL010300_SupplierFile AS PL ON NAF.SzallitoKod = PL.SupplierCode LEFT OUTER JOIN
                         ScaCompDB.dbo.VNB_SC010300_Stockfile AS SC ON NAS.Cikkszam = SC.StockCode LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_SCUN0300_UnitCode AS SCU ON SC.UnitCodeStoc = SCU.UnitCode LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_SYCD0300_DevizaNevek AS D ON SC.CurrCodePurc = D.CurrencyCode
WHERE        (NAS.Status = 0)
ORDER BY NAF.ID</CommandText>
