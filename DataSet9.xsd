            <MainSource>
              <DbSource ConnectionRef="ScaCompDBConnectionString (Settings)" DbObjectName="ScaCompDB.dbo.VNB_PC030300_POLineFile" DbObjectType="View" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        L.PurchaOrdNo, MAX(L.SupplierOrderLineRef) AS SupplierOrderLineRef, MAX(P.SuppCodBuyer) AS SuppCodBuyer, MAX(S.SupplierName) AS SupplierName, MAX(P.OrderValue) AS OrderValue, MAX(P.OrderDate) 
                         AS OrderDate, MAX(A.RendelesSzam) AS RendelesSzam, MAX(F.Kerelmezo) AS Kerte, MAX(F.Status) AS Status, COUNT(L.StockCode) AS Lines, MAX(D.CurrencyShortName) AS Deviza
FROM            VNB_PC030300_POLineFile AS L LEFT OUTER JOIN
                         VF_PC010300_POMasterFile AS P ON L.PurchaOrdNo = P.PurchOrderNo LEFT OUTER JOIN
                         VF_PL010300_SupplierFile AS S ON P.SuppCodBuyer = S.SupplierCode LEFT OUTER JOIN
                         Fusetech.dbo.NBAnyagigenylo AS A ON L.SupplierOrderLineRef = CAST(A.RendelesSzam AS nvarchar) + CAST(A.RendSorszam AS nvarchar) LEFT OUTER JOIN
                         Fusetech.dbo.NBAnyagigenyloFej AS F ON A.RendelesSzam = F.ID LEFT OUTER JOIN
                         VF_SYCD0300_DevizaNevek AS D ON P.CurrCode = D.CurrencyCode_2
WHERE        (A.Status = 2)
GROUP BY L.PurchaOrdNo
ORDER BY L.PurchaOrdNo</CommandText>
