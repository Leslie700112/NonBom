          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="VNB_PC030300_POLineFileTableAdapter" GeneratorDataComponentClassName="VNB_PC030300_POLineFileTableAdapter" Name="VNB_PC030300_POLineFile" UserDataComponentName="VNB_PC030300_POLineFileTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ScaCompDBConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        PC03.PurchaOrdNo, PC01.SuppCodBuyer, PL.SupplierName, CONVERT(date, PC03.DelDateCommi) AS VisszIg, PC03.LineNumber, PC03.StockCode, PC03.DescriptLine1, PC03.DescriptLine2, PC03.UnitPrice, 
                         D.CurrencyShortName AS CurrencyCode, RTRIM(SCU.Description) AS Description, PC03.PCQtyOrdered, PC03.PCQtyReceive, PC03.PCQtyInvoice, PC01.OurReference, PL.AddressLine1, SC.UserDefined8, 
                         PC03.StockCode + PC03.DescriptLine1 + ISNULL(PC03.DescriptLine2, N'') + ISNULL(SC.InternRem1, N'') + ISNULL(SC.InternRem2, N'') AS Kereso, 
                         PC03.PurchaOrdNo + PC01.SuppCodBuyer + PL.SupplierName AS Kereso2, SC.InternRem1, SC.InternRem2, ISNULL(FNB.Menny, 0) AS Menny
FROM            VNB_PC030300_POLineFile AS PC03 LEFT OUTER JOIN
                         VF_PC010300_POMasterFile AS PC01 ON PC03.PurchaOrdNo = PC01.PurchOrderNo LEFT OUTER JOIN
                         VF_PL010300_SupplierFile AS PL ON PC01.SuppCodBuyer = PL.SupplierCode LEFT OUTER JOIN
                         VF_SCUN0300_UnitCode AS SCU ON PC03.UnitCode = SCU.UnitCode LEFT OUTER JOIN
                         VF_SC010300_Stockfile AS SC ON PC03.StockCode = SC.StockCode LEFT OUTER JOIN
                         VF_SYCD0300_DevizaNevek AS D ON PC03.CurrencyCode = D.CurrencyCode LEFT OUTER JOIN
                         Fusetech.dbo.NBBeszerzesBevet_GroupPOLine AS FNB ON PC03.PurchaOrdNo = FNB.BeszRendSzam AND PC03.LineNumber = FNB.Sorszam LEFT OUTER JOIN
                         Fusetech.dbo.NBAnyagigenylo AS NBA ON PC03.SupplierOrderLineRef = CAST(NBA.RendelesSzam AS nvarchar) + CAST(NBA.RendSorszam AS nvarchar)
WHERE        (NBA.Tipus = '0' OR
                         NBA.Tipus = '2') AND (PC03.PCQtyOrdered - PC03.PCQtyReceive &gt; 0)
ORDER BY PC03.PurchaOrdNo, PC03.LineNumber</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
