            <MainSource>
              <DbSource ConnectionRef="FusetechConnectionString1 (Settings)" DbObjectName="Fusetech.dbo.NBSzolgaltatas" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        NB.RendelesSzam AS ID, MAX(NB.Szolgaltato) AS Szolgaltato, MAX(PL.SupplierName) AS SupplierName, MAX(NB.Munkahely) AS Munkahely, MAX(MT.Sor + ' ' + MT.Allomas + ' ' + MT.Eszkoz) AS GSZNev, 
                         MAX(NB.BeruszamRACF) AS BeruszamRACF, MAX(NB.Megjegyzes) AS Megjegyzes, MAX(NB.Igenylo) AS Igenylo, MAX(NB.RendIdo) AS RendIdo, MAX(NB.Statusz) AS Statusz, MAX(NB.Engedelyezo) 
                         AS Expr1
FROM            NBSzolgaltatas AS NB LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_PL010300_SupplierFile AS PL ON NB.Szolgaltato = PL.SupplierCode LEFT OUTER JOIN
                         MTGeplista AS MT ON NB.GepSzerszam = MT.Fusetech_Kod
WHERE        (NB.Statusz = '1') OR
                         (NB.Statusz = '2')
GROUP BY NB.RendelesSzam</CommandText>
