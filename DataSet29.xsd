              <DbSource ConnectionRef="FusetechConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        F.BizonylatSzam, F.Cikkszam, SC01.Description1, SC01.Description2, SC01.InternRem1, F.Raktar, F.Polc, F.Mennyiseg, E.TextDescription AS Terület, F.KtsghKod, F.KtsghNev, F.Igenylo, I.Nev AS TextDescription, F.Kiadta, 
                         K.TextDescription AS Expr1, F.Megjegyzes, ISNULL(F.GepSzerszam, N'') AS GepKod, ISNULL(G.Sor + ' ' + G.Allomas + ' ' + G.Eszkoz, N'') AS GepMegnevezes, ISNULL(F.FelhIdeje, N'2017-01-01') AS FelhIdeje, 
                         RTRIM(U.Description) AS MEgys, I.TSz
FROM            NBFelhasznalas AS F LEFT OUTER JOIN
                         ScaCompDB.dbo.VNB_SC010300_Stockfile AS SC01 ON F.Cikkszam = SC01.StockCode LEFT OUTER JOIN
                         HRTranzakciok_Kod_Group AS I ON RTRIM(F.Igenylo) = RTRIM(REPLACE(I.Kod, '*', '')) LEFT OUTER JOIN
                         DolgKodok AS K ON RTRIM(F.Kiadta) = RTRIM(K.Key1) LEFT OUTER JOIN
                         MTGeplista AS G ON F.GepSzerszam = G.Fusetech_Kod LEFT OUTER JOIN
                         ScaCompDB.dbo.VNB_SY240300_EXT_termcsop AS E ON SC01.ExtProdGroup = E.Key1 LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_SCUN0300_UnitCode AS U ON SC01.UnitCodeStoc = U.UnitCode
ORDER BY F.Cikkszam</CommandText>
