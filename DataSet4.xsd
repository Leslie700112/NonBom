                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        VNB_ST080300_PurchaseStat.SupplierCode, VNB_ST080300_PurchaseStat.OrderNo, VNB_ST080300_PurchaseStat.DeliveryDate, 
                         VNB_ST080300_PurchaseStat.StockCode, VNB_ST080300_PurchaseStat.ProductGroup, VNB_ST080300_PurchaseStat.AltProdGroup, 
                         VNB_ST080300_PurchaseStat.PriceLCU, VNB_ST080300_PurchaseStat.Price, VNB_ST080300_PurchaseStat.QtySC, VNB_ST080300_PurchaseStat.CurrencyCode, 
                         VNB_ST080300_PurchaseStat.InvoiceNo, VF_PL010300_SupplierFile.SupplierName, VF_SY240300_termcsop.TextDescription, 
                         VF_PL030300_PurchaseLedger.InternRefNo, PC43.DelivNoteNo, VNB_ST080300_PurchaseStat.InvoiceDate1
FROM            VNB_ST080300_PurchaseStat LEFT OUTER JOIN
                         VF_PL030300_PurchaseLedger ON VNB_ST080300_PurchaseStat.InvoiceNo = VF_PL030300_PurchaseLedger.InvoiceNo LEFT OUTER JOIN
                         VF_SY240300_termcsop ON VNB_ST080300_PurchaseStat.ProductGroup = VF_SY240300_termcsop.Key1 LEFT OUTER JOIN
                         VF_PL010300_SupplierFile ON VNB_ST080300_PurchaseStat.SupplierCode = VF_PL010300_SupplierFile.SupplierCode LEFT OUTER JOIN
                         VF_PC430300 AS PC43 ON VNB_ST080300_PurchaseStat.InvoiceNo = PC43.InvoiceNo_Last AND 
                         VNB_ST080300_PurchaseStat.BatchIDDeliv = PC43.BatchNumber
WHERE        (VNB_ST080300_PurchaseStat.StockCode = @v_cikk)
ORDER BY VNB_ST080300_PurchaseStat.DeliveryDate DESC</CommandText>
