          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="NBSzolgaltatasTableAdapter" GeneratorDataComponentClassName="NBSzolgaltatasTableAdapter" Name="NBSzolgaltatas" UserDataComponentName="NBSzolgaltatasTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="FusetechConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        SF.ID, NB.RendSorszam, NB.Szolgaltato, PL.SupplierName, NB.Munkahely, NB.GepSzerszam, MT.Sor + ' ' + MT.Allomas + ' ' + MT.Eszkoz AS GSZNev, NB.BeruszamRACF, NB.Megjegyzes, NB.Igenylo, 
                         NB.Statusz, NB.RendIdo, NB.Engedelyezo, NB.EngIdo, NB.IDS, NB.Ar, NB.Deviza, NB.SzolgTipus
FROM            NBSzolgaltatas AS NB LEFT OUTER JOIN
                         ScaCompDB.dbo.VF_PL010300_SupplierFile AS PL ON NB.Szolgaltato = PL.SupplierCode LEFT OUTER JOIN
                         MTGeplista AS MT ON NB.GepSzerszam = MT.Fusetech_Kod LEFT OUTER JOIN
                         NBSzolgaltatasFej AS SF ON NB.RendelesSzam = SF.ID
WHERE        (NB.Statusz &gt; 0) AND (NB.Statusz &lt; 3)
ORDER BY SF.ID, NB.RendSorszam</CommandText>
