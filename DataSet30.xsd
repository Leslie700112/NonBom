                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        PO.PurchaOrdNo, PO.LineNumber, PO.StockCode, PO.DescriptLine1, PO.DescriptLine2, PO.UnitPrice, PO.PCQtyOrdered, PO.PCQtyReceive, PO.PCQtyInvoice, PO.DelDateCommi, PO.DelDateActua, 
