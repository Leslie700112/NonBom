                         VF_PC010300_POMasterFile AS PC01 ON PO.PurchaOrdNo = PC01.PurchOrderNo INNER JOIN
                         PC190300 AS PC ON PO.PurchaOrdNo = PC.PC19001 AND PO.LineNumber = PC.PC19002 LEFT OUTER JOIN
                         VF_PL010300_SupplierFile AS PL ON PC01.SuppCodBuyer = PL.SupplierCode LEFT OUTER JOIN
                         VF_SCUN0300_UnitCode AS SCU ON PO.UnitCode = SCU.UnitCode LEFT OUTER JOIN
